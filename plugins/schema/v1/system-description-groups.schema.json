{
  "$schema": "http://json-schema.org/draft-04/schema#",

  "type": "array",
  "items": {
    "type": "object",
    "required": ["name", "password", "gid", "users"],
    "properties": {
      "name": {
        "type": "string"
      },
      "password": {
        "type": "string"
      },
      "gid": {
        "type": "integer",
        "minimum": 0
      },
      "users": {
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    }
  }
}

